<?php

/**
 * @file
 * Hooks and customizations for the farm_asset module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function farm_asset_help($route_name, RouteMatchInterface $route_match) {
  $output = '';

  // Main module help for the farm_asset module.
  if ($route_name == 'help.page.farm_asset') {
    $output = '';
    $output .= '<h3>' . t('About') . '</h3>';
    $output .= '<p>' . t('Provides farm asset entity') . '</p>';
  }

  return $output;
}
